<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.ViewLifeActivity">

    <view
        class="cn.dozyx.template.view.ViewLifeActivity$LifeViewLinearLayout"
        android:id="@+id/life_view_group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <view
            class="cn.dozyx.template.view.ViewLifeActivity$ViewLifeView"
            android:id="@+id/life_view"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:background="@color/colorAccent" />
    </view>

</androidx.constraintlayout.widget.ConstraintLayout>